{
  "name": "@rule34/agent",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "bun run server.ts",
    "build": "bun build ./index.ts --outdir ./dist --target=node --external=@langchain/langgraph --external=@langchain/openai --external=uuid --external=zod && tsc --emitDeclarationOnly --outDir ./dist",
    "dev": "bun --watch server.ts"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.5",
    "@hono/zod-validator": "^0.7.0",
    "@langchain/langgraph": "^0.3.7",
    "@langchain/openai": "^0.5.18",
    "@supabase/supabase-js": "^2.50.4",
    "hono": "^4.6.14",
    "uuid": "^11.1.0",
    "zod": "^3.24.4"
  }
}
